    "semver": "^7.5.4",
    "shelljs": "^0.8.5",
    "stylus": "^0.59.0",
    "vue": "^2.7.14",
    "vue-jest": "^3.0.7",
    "vue-server-renderer": "^2.7.14",
    "vue-template-compiler": "^2.7.14"
  }
